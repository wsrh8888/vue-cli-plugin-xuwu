{
  "name": "xuwu",
  "version": "1.0.6",
  "description": "主动格式化vue-cli工程基础模版插件",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix ./generator  --ext .vue,.js,.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "prettier",
    "eslint",
    "husky",
    "lib-flexible",
    "vconsole",
    "cross-env",
    "babel-plugin-transform-remove-console"
  ],
  "author": "Robert",
  "license": "ISC",
  "files": [
    "generator",
    "index.js",
    "prompts.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/dawwdadfrf/vue-cli-plugin-init-structure.git"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  }
}
