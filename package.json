{
  "name": "vue-cli-plugin-xuwu",
  "version": "1.3.1",
  "description": "通过命令行的方式初始化项目模版，从而完成项目的基础配置",
  "main": "index.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build": "tsc && npm run lint",
    "lint": "eslint --fix ./prompts.js ./generator ./prompts  --ext .vue,.js,.ts",
    "test": "node '/Users/wymac/Documents/source/my-source/xuwu/xuwu/generator/static/ast.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "prettier",
    "eslint",
    "husky",
    "lib-flexible",
    "vconsole",
    "cross-env",
    "babel-plugin-transform-remove-console",
    "格式化代码",
    "vite配置",
    "vue3配置"
  ],
  "author": "Robert",
  "license": "ISC",
  "files": [
    "generator",
    "prompts",
    "index.js",
    "prompts.js",
    "README.md",
    "RELEASE.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wsrh8888/vue-cli-plugin-xuwu.git"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.5",
    "vuepress": "^2.0.0-beta.53"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/types": "^7.18.13",
    "chalk": "^4.1.2"
  }
}
