{
  "name": "vue-cli-plugin-xuwu",
  "version": "1.0.6",
  "description": "xuwu模板助手，通过插件的方式对vue生态圈的所有项目进行高效，便捷的扩展",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix ./generator  --ext .vue,.js,.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "prettier",
    "eslint",
    "husky",
    "lib-flexible",
    "vconsole",
    "cross-env",
    "babel-plugin-transform-remove-console",
    "基础建设",
    "扩展",
    "完善"
  ],
  "author": "Robert",
  "license": "ISC",
  "files": [
    "generator",
    "index.js",
    "prompts.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/dawwdadfrf/vue-cli-plugin-xuwu.git"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.3.0",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-cli-plugin-init-structure": "~1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  }
}
